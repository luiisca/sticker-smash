{
    "expo": {
        "name": "sticker-smash",
        "slug": "sticker-smash",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.luiisca.stickersmash"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "extra": {
            "eas": {
                "projectId": "a022d4bc-d932-45a8-b18b-b08d2d27bb58"
            }
        },
        "plugins": [
            [
                "@react-native-google-signin/google-signin"
            ]
        ]
    }
}
